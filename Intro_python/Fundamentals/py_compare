function ifstatement(){
    var myvariable = 50;
    var bool = true;
    if (myvariable == 99999 && false){
        my variable--;
        console.log("to infinity");
    }
    else if(my variable == 50){
        myvariable++;
        console.log("and beyond");
    }
}

use def instead of function
